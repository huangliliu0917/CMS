    "editorHTML": "\r\n<div class=\"editor\" style=\"display:block;\" xmlns:th=\"http://www.thymeleaf.org\">\r\n\r\n    <script type=\"text/javascript\" th:if=\"${@environment.acceptsProfiles('test')}\" th:inline=\"javascript\">\r\n        $(function () {\r\n            var properties = widgetProperties(this);\r\n            $(\".savePagingWidget\").click(function () {\r\n                var pro = $(this).parent().parent();\r\n                $.each($(pro).find(\".pagingPageCount\"), function (i, obj) {\r\n                    properties.pageCount = $(obj).val();\r\n                });\r\n                $.each($(pro).find(\".pagingTColor\"), function (i, obj) {\r\n                    properties.pagingTColor = $(obj).val();\r\n                });\r\n                $.each($(pro).find(\".pagingHColor\"), function (i, obj) {\r\n                    properties.pagingHColor = $(obj).val();\r\n                });\r\n                console.log(properties);\r\n            });\r\n\r\n\r\n        });\r\n        function testUpdate() {\r\n            $.each($(\".pagingPageCount\"), function (i, obj) {\r\n                $(obj).val(\"40\");\r\n            });\r\n            $.each($(\".pagingTColor\"), function (i, obj) {\r\n                $(obj).val(\"#222222\");\r\n            });\r\n            $.each($(\".pagingHColor\"), function (i, obj) {\r\n                $(obj).val(\"#333333\");\r\n            });\r\n        }\r\n    </script>\r\n    <div class=\"conf-header\">\r\n        <p>璁剧疆</p>\r\n        <button class=\"btn btn-danger\">鍙栨秷</button>\r\n        <button class=\"btn btn-success savePagingWidget\">淇濆瓨</button>\r\n    </div>\r\n    <div class=\"conf-body\" style=\"display:block;\">\r\n\r\n        <div>\r\n            <h3>\r\n                <i class=\"icon-puzzle\"></i><b></b>閫夋嫨缁勪欢鏍峰紡\r\n            </h3>\r\n\r\n            <div class=\"styles\">\r\n                <div class=\"style-item\">\r\n                    <!-- 鏍峰紡缂╃暐鍥�-->\r\n                    <div class=\"item-box\" data-styleId=\"pagingWidgetDefaultStyle\">\r\n                        <img w:src=\"thumbnail/defaultStyleThumbnail.png\">\r\n\r\n                        <p>榛樿</p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!--缂栬緫瑙嗗浘-->\r\n        <div>\r\n            <h3>\r\n                <i class=\"icon-puzzle\"></i><b></b>璁剧疆缁勪欢鍙傛暟\r\n            </h3>\r\n\r\n            <div class=\"copyrightBox borderBoxs\">\r\n                <form class=\"form-horizontal\">\r\n                    <div class=\"form-group\">\r\n                        <label class=\"col-md-3 control-label\">姣忛〉鏄剧ず鏁伴噺</label>\r\n\r\n                        <div class=\"col-md-3\">\r\n                            <input type=\"text\" class=\"form-control pagingPageCount\" value=\"20\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label class=\"col-md-3 control-label\">鏂囧瓧棰滆壊</label>\r\n\r\n                        <div class=\"col-md-3\">\r\n                            <input type=\"color\" class=\"form-control pagingTColor\">\r\n                        </div>\r\n                        <label class=\"col-md-3 control-label\">閫変腑棰滆壊</label>\r\n\r\n                        <div class=\"col-md-3\">\r\n                            <input type=\"color\" class=\"form-control pagingHColor\">\r\n                        </div>\r\n                    </div>\r\n\r\n                </form>\r\n            </div>\r\n        </div>\r\n\r\n        <!--/缂栬緫瑙嗗浘-->\r\n    </div>\r\n</div>\r\n\r\n\r\n",
